class Solution {
public:
    string getPermutation(int n, int k) {
        int arr[11];
        for(int i = 0 ; i < n ; i++){
            arr[i] = i + 1;
        }
        for(int i = 1 ; i < k ; i++){
            next_permutation(arr , arr + n);
        }
        string s1;
        for(int i = 0 ; i < n ; i++){
            s1 = s1 + to_string(arr[i]);
        }
        return s1;
    }
};
